{
  "name": "Zwiftalizer",
  "description": "Zwift log analyzer web app",
  "version": "1.2.2",
  "build": "1",
  "license": "Prioprietary",
  "repository": "git@github.com:mhanney/zwiftalizer.git",
  "private": false,
  "author": {
    "name": "mhanney",
    "url": "https://github.com/mhanney/zwiftalizer"
  },
  "scripts": {
    "deploy": "s3-upload aws-upload.conf.js",
    "dev": "webpack-dev-server",
    "lint": "eslint src/js/*.jsx",
    "posttest": "rm .babelrc",
    "pretest": "cp .testbabelrc .babelrc",
    "release": "webpack --progress --profile --colors",
    "test": "babel-tape-runner tests/mapAntSearches.js",
    "install-local-modules": "npm install --prefix local_modules file:../zwiftalizer-antplus-devices"
  },
  "optionalDependencies": {
    "fsevents": "^1.0.12"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "bundle-loader": "^0.5.0",
    "classnames": "^2.2.5",
    "connect": "~3.4.0",
    "css-loader": "^0.23.1",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "merge": "^1.2.0",
    "raw-loader": "^0.5.1",
    "s3-upload": "^0.1.0",
    "style-loader": "^0.13.1",
    "tape": "^4.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.5.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "uuid": "^2.0.1",
    "react-ga": "^2.1.2",
    "ramda": "^0.22.1",
    "underscore": "^1.8.3",
    "bootstrap.native": "^1.0.1",
    "d3": "^3.5.17",
    "d3-array": "^0.8.1",
    "d3-axis": "^0.3.0",
    "d3-ease": "^0.7.0",
    "d3-format": "^0.5.1",
    "d3-interpolate": "^0.7.0",
    "d3-scale": "^0.9.3",
    "d3-scale-chromatic": "^0.3.0",
    "d3-selection": "^0.7.3",
    "d3-shape": "^0.7.1",
    "d3-time": "^0.2.5",
    "d3-time-format": "^2.0.5",
    "d3-transition": "^0.2.8",
    "file-loader": "^0.7.2",
    "flexbox-react": "^4.0.0",
    "history": "^1.13.1",
    "immutable": "^3.6.4",
    "imports-loader": "^0.6.5",
    "invariant": "^2.2.0",
    "json-loader": "^0.5.4",
    "loadash": "0.0.1",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "moment-duration-format": "^1.3.0",
    "node-uuid": "^1.4.7",
    "paths-js": "^0.4.2",
    "pondjs": "^0.7.2",
    "qwest": "^4.1.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.3.2",
    "react-dom-confetti": "0.0.7",
    "react-dropzone": "^3.5.3",
    "react-easy-chart": "^0.1.13",
    "react-faux-dom": "^2.7.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-tabs": "^0.7.0",
    "react-timeseries-charts": "^0.10.2",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "sprintf-js": "^1.0.3"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "settings": {
      "import/parser": "babel-eslint",
      "import/resolve": {
        "moduleDirectory": [
          "node_modules",
          "src"
        ]
      }
    },
    "rules": {
      "no-console": 0,
      "max-len": [
        2,
        140,
        2
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "never",
          "objects": "never",
          "imports": "never",
          "exports": "never",
          "functions": "ignore"
        }
      ],
      "linebreak-style": 0,
      "arrow-parens": 0,
      "arrow-body-style": [
        "error",
        "always"
      ]
    }
  }
}
